{
  "stassets": {
    "verbose": true,
    "root": "./src/client",
    "vendors": {
      "prefix": [
        "node_modules"
      ],
      "js": [
        "rupert-plugin-angular/node_modules/angular-builds/angular-route.js"
      ],
      "css": []
    },
    "angular": {
      "useJquery": true
    },
    "scripts": {
      "types": [
        "animation"
      ]
    }
  },
  "tls": false,
  "server": {
    "root": "./src/server",
    "extensions": [
      "js",
      "coffee"
    ],
    "routing": [
      "allow",
      "**/*route"
    ]
  },
  "static": {
    "phones": "src/phones/",
    "img": "src/img/"
  }
}
